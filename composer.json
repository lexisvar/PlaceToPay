{
    "name": "lexisvar/place_to_pace",
    "type": "project",
    "description": "The Payum extension. Place To Pay Extension for Payum",
    "keywords": ["payment"],
    "homepage": "http://payum.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexis Vargas",
            "email": "lexisvar@gmail.com"
        },
        {
            "name":     "Handy Commerce",
            "homepage": "http://handycommerce.co/"
        }
    ],
    "require": {
        "payum/core": "^1.3"
